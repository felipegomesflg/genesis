{
  "name": "dot-genesis",
  "version": "0.0.0",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "electron . && node server.js",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "package": "electron-packager ./app ha --platform=win32 --arch=x64  --asar=true",
    "build-electron": "ng build --base-href . && cp src/electron/* dist",
    "build-electron-win": "ng build --base-href . && copy src\\electron\\* dist",
    "electron": "npm run build-electron && electron dist",
    "electron-win": "npm run build-electron-win && electron dist",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "db": "cp ./DB/_database.sqlite ./DB/database.sqlite",
    "db-win": "copy .\\DB\\_database.sqlite .\\DB\\database.sqlite"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.5",
    "@types/jquery": "^3.2.12",
    "angular-2-local-storage": "^1.0.1",
    "cors": "^2.8.4",
    "body-parser": "^1.18.2",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "electron": "^1.7.11",
    "electron-packager": "^10.1.1",
    "express": "^4.16.2",
    "jquery": "^3.1.1",
    "knex": "^0.14.2",
    "mongodb": "^2.2.33",
    "rxjs": "^5.1.0",
    "sqlite3": "^3.1.13",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.6.5",
    "@angular/compiler-cli": "^4.0.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "electron-rebuild": "^1.7.3",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0"
  }
}
